    <div class="container">
      <div class="page-header">
        <p class="lead">GENOME LINK example</p>
      </div>

      {{#if authorize_url}}

        <a href="{{authorize_url}}" class="btn btn-default">Connect with genome</a>
      {{/if}}

        <table class="table">
          <thead>
            <tr>
              <th>Name</th>
              <th>Genetic insights</th>
            </tr>
          </thead>
          <tbody>
            {{#each reports}}

            <tr>
              <th scope="row">{{this.phenotype.display_name}}</th>
              <td>{{this.summary.score}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>

    </div>
